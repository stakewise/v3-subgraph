{
  "name": "v3-subgraph",
  "version": "0.1.0",
  "description": "Subgraph for the StakeWise Protocol",
  "repository": "https://github.com/stakewise/v3-subgraph",
  "license": "AGPL-3.0-only",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "---SCRIPTS---": "",
    "generate-abis": "node ./scripts/generateAbis.js",
    "deploy": "node ./scripts/deploy.js",
    "---GOERLI---": "",
    "generate-yaml:goerli": "mustache src/config/goerli.json src/subgraph.template.yaml > src/subgraph-goerli.yaml",
    "generate-tests-yaml:goerli": "mustache src/config/goerli.json matchstick.template.yaml > matchstick.yaml",
    "generate-types:goerli": "graph codegen src/subgraph-goerli.yaml",
    "generate-wasm:goerli": "graph build src/subgraph-goerli.yaml -o build/goerli",
    "prepare:goerli": "npm run generate-yaml:goerli && npm run generate-types:goerli",
    "build:goerli": "npm run prepare:goerli && npm run generate-wasm:goerli",
    "test:goerli": "npm run prepare:goerli && npm run generate-tests-yaml:goerli && graph test",
    "deploy-hosted:goerli": "npm run prepare:goerli && npm run deploy network:goerli node:hosted",
    "deploy-local:goerli": "npm run prepare:goerli && npm run deploy network:goerli node:local"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "0.56.0",
    "@graphprotocol/graph-ts": "0.31.0",
    "@types/node": "20.5.6",
    "@typescript-eslint/eslint-plugin": "6.4.1",
    "@typescript-eslint/parser": "6.4.1",
    "dotenv": "16.3.1",
    "eslint": "8.48.0",
    "eslint-config-prettier": "9.0.0",
    "husky": "8.0.3",
    "lint-staged": "14.0.1",
    "matchstick-as": "0.5.2",
    "mustache": "4.2.0",
    "prettier": "3.0.2",
    "typescript": "5.2.2"
  },
  "engines": {
    "node": "18"
  },
  "lint-staged": {
    "*.ts": "eslint --cache --fix",
    "*.{ts,yaml,md,json}": "prettier --write"
  }
}
