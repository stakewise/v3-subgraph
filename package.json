{
  "name": "stakewise-subgraphs",
  "version": "0.0.1",
  "description": "Subgraph for the StakeWise Protocol",
  "repository": "https://github.com/stakewise/v3-subgraph",
  "license": "AGPL-3.0-only",
  "private": true,
  "scripts": {
    "---GOERLI---": "",
    "generate-yaml:goerli": "mustache src/config/goerli.json src/subgraph.template.yaml > src/subgraph-goerli.yaml",
    "generate-types:goerli": "graph codegen src/subgraph-goerli.yaml",
    "generate-wasm:goerli": "graph build src/subgraph-goerli.yaml",
    "prepare:goerli": "npm run generate-yaml:goerli && npm run generate-types:goerli",
    "build:goerli": "npm run prepare:goerli && npm run generate-wasm:goerli",
    "deploy:goerli": "",
    "---GRAPH---": "",
    "graph:auth": "graph auth https://api.thegraph.com/deploy/ $GRAPH_TOKEN"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "0.28.1",
    "@graphprotocol/graph-ts": "0.26.0",
    "@types/node": "17.0.22",
    "@typescript-eslint/eslint-plugin": "5.16.0",
    "@typescript-eslint/parser": "5.16.0",
    "eslint": "8.11.0",
    "mustache": "4.2.0",
    "typescript": "4.6.2"
  }
}
